<%# debug @category %>

<section class="categories-show">

  <header class="page-header">
    <h1>
      <%# raise @category.inspect  %>
      <%= @category.name %>!!!
      <small>(<%= pluralize @products.size, 'product' %>)</small>
    </h1>
  </header>

  <div class="products">
    <%= render @products %>
  </div>

</section>